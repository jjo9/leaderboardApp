<% if ( tabela.length == 0 ){ %>
  <h2>Challenges were not found</h2>
<% } else { %>
  <% for ( var i = 0;  i < tabela.length; i++) { %>
    <h3>Perguntas resolvidas</h3>
    
      <% for( var ii = 0; ii < tabela[i].perguntasResolvidas.length; ii++){ %>
        <tr class="table-light">
          <th scope="row">#<%= ii +1 %></th>
          <h1><%= tabela[i].perguntasResolvidas[ii].perguntaID2 %></h1>
          <td>
            <form action="/findUser/" method="post">
              <button type="submit" name="username" value="<%= tabela[i].perguntasResolvidas[ii]._id %>" class="btn-link"><%= tabela[i].perguntasResolvidas[ii].perguntaID2 %></button>
            </form>
            </td>
        </tr>
 
    <% } %>
  <% } %>
<% } %>

