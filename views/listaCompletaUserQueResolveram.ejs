  <h1>Todos os User que Resolveveram o Desafio</h1>
  <h1>Por este igual ao challangeInfo.ejs para que o estilo de amostra de info não seja diferente </h1>
  
  
  <% if ( tabela.length == 0 ){ %>
    <h2>O challange não foi encontrado</h2>
  <% } else { %>
    <% for ( var i = 0;  i < tabela.length; i++) { %>
  
    <div class="jumbotron">
    <h1 class="display-6" style="display: inline" ><strong>ID do desafio:</strong> <%= tabela[i].IDdesafio %></h1> 
  
    <% var options = { year: 'numeric', month: 'long', day: 'numeric' }; %>
    <h5 class="since">criado em: <%= tabela[i].dataCriacao.toLocaleString('pt-BR',options) %> </h5> <br>
  
    <font size="6"><span class="badge badge-pill badge-info">Pontos: <%= tabela[i].Pontos %></span></font>
      <h2>Rating: <%= tabela[i].ratingScore %></h2>
      <% var blueRating = tabela[i].ratingScore; %>
      <% var greyRating = 100 - blueRating; %>
      <%  %>
    <div class="progress tiny">
      <div class="progress-bar blueRating" role="progressbar" style="width: <%= blueRating %>%" aria-valuenow="<%= blueRating %>" aria-valuemin="0" aria-valuemax="100"></div>
      <div class="progress-bar greyRating" role="progressbar" style="width: <%= greyRating %>%" aria-valuenow="<%= greyRating %>" aria-valuemin="0" aria-valuemax="100"></div>
    </div>
    <h4>Solvs: <%= tabela[i].usersQueResolveram_size %></h4>
    <hr class="my-4">
    <p><%= tabela[i].IDdesafio %></p>
    <p>Categoria: <%= tabela[i].Categoria %></p>
    <p>Dificuldade: <%= tabela[i].Dificuldade %></p>  
    <p>Recursos: <%= tabela[i].Recursos %></p>  
    <p>Titulo: <%= tabela[i].DesafioTitulo %></p>
    <p>Descrição: <%= tabela[i].DesafioDescricao %></p>  
    <p>username do autor: <%= tabela[i].AutorUsername %></p>  
    <p>mongoo _id do autor: <%= tabela[i].Autor %></p>  
    
      <table class="table table-hover">
      <thead>
        <tr>
          <th scope="col">Place:</th>
          <th scope="col">User ID</th>
          <th scope="col">Time</th>
        </tr>
      </thead>
      <tbody>
              
        <% for( var ii = 0; ii < tabela[i].usersQueResolveram_size; ii++){ %>
          <tr class="table-light">
            <th scope="row">#<%= ii +1 %></th>
            <td>
              <form action="/findUser/" method="post">
                <button type="submit" name="username" value="<%= tabela[i].usersQueResolveram[ii].userID %>" class="btn-link"><%= tabela[i].usersQueResolveram[ii].userID2 %></button>
              </form>
              </td>
            <td><%= tabela[i].usersQueResolveram[ii].data.toLocaleString('pt-BR',options) %></td>
          </tr>
        <% } %>
    </tbody>
  </table> 
  
  </div>
    
    <% } %>
  
  <% } %>
